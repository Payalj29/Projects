from time import *
import random as r
def mistakes(partest,usertest):
  error=0
  for i in range(len(partest)):
    try:
      if partest[i]!=usertest[i]:
        error=error+1
    except:
      error=error+1
  return error

def speed_time(time_s,time_e,userinput):
  time_delay=time_e-time_s
  time_R=round(time_delay,2)
  speed=len(userinput)/time_R
  return round(speed)

